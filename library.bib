@article{palluk_novo_2018-1,
	title = {De novo {DNA} synthesis using polymerase-nucleotide conjugates},
	copyright = {2018 Nature Publishing Group},
	issn = {1546-1696},
	url = {https://www.nature.com/articles/nbt.4173},
	doi = {10.1038/nbt.4173},
	abstract = {Oligonucleotides are almost exclusively synthesized using the nucleoside phosphoramidite method, even though it is limited to the direct synthesis of ∼200 mers and produces hazardous waste. Here, we describe an oligonucleotide synthesis strategy that uses the template-independent polymerase terminal deoxynucleotidyl transferase (TdT). Each TdT molecule is conjugated to a single deoxyribonucleoside triphosphate (dNTP) molecule that it can incorporate into a primer. After incorporation of the tethered dNTP, the 3′ end of the primer remains covalently bound to TdT and is inaccessible to other TdT–dNTP molecules. Cleaving the linkage between TdT and the incorporated nucleotide releases the primer and allows subsequent extension. We demonstrate that TdT–dNTP conjugates can quantitatively extend a primer by a single nucleotide in 10–20 s, and that the scheme can be iterated to write a defined sequence. This approach may form the basis of an enzymatic oligonucleotide synthesizer.},
	language = {en},
	urldate = {2018-06-19},
	journal = {Nature Biotechnology},
	author = {Palluk, Sebastian and Arlow, Daniel H. and Rond, Tristan de and Barthel, Sebastian and Kang, Justine S. and Bector, Rathin and Baghdassarian, Hratch M. and Truong, Alisa N. and Kim, Peter W. and Singh, Anup K. and Hillson, Nathan J. and Keasling, Jay D.},
	month = jun,
	year = {2018},
}

@phdthesis{vanbavinckhove_optics_2013-2,
	address = {S.l]; Amsterdam},
	title = {Optics measurements and corrections for colliders and other storage rings},
	abstract = {Glenn Vanbavinckhove ontwikkelde onder meer een nieuw algoritme, de segment-by-segment-techniek, om sterke lokale fouten voor belangrijke optische parameters te identificeren in versnellers en andere opslagringen. Vanbavinckhove presenteert methodes en technieken voor de meting en correctie van de lineaire optica in versnellers en andere opslagringen. De segment-by-segment-techniek bleek zeer nuttig te zijn tijdens de ingebruikname van de LHC (Large Hadron Collider, ondergrondse deeltjesversneller). Door een meer geautomatiseerde manier van het identificeren van lokale fouten, bijvoorbeeld door gebruik te maken van een lokaal matching script of lokale responsmatrix, kan de segment-by-segment-techniek verbeterd worden. Ook beschrijft Vanbavinckhove in detail de modus operandi voor het meten en corrigeren van de optiek. De turn by turn-data verkregen in de beam position monitors (monitoren die de positie van de bundel waarnemen) werden gebruikt om de optische parameters te meten.},
	language = {English},
