@article{bruce_hl-lhc_nodate,
	title = {{HL}-{LHC} {Operational} {Scenarios} for {Pb}-{Pb} and p-{Pb} {Operation}},
	abstract = {This report describes the running scenario for operation with Pb-Pb and p-Pb collisions in the HL-LHC. The expected beam parameters are described, starting from the beams delivered from the injectors, as well as possible filling schemes. The machine configuration is also discussed, in terms of optics and settings of critical components such as collimators. Finally, the future luminosity performance in typical fills for Pb-Pb and p-Pb collisions is simulated, and the expected integrated luminosity over the HL-LHC physics programme is calculated and compared to the requirements from the experiments.},
