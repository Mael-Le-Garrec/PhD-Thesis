	note = {Number: 5
Conference Name: IEEE Transactions on Applied Superconductivity},
	keywords = {Production, Coils, Strain, 11 T Dipole, Collared Coil Production, Conductors, field quality, geometry, Geometry, Harmonic analysis, harmonics, Hi-Lumi, Magnetic field measurement, Nb ₃ Sn},
	pages = {1--6},
}

@article{garcia-matos_fabrication_2021-1,
	title = {Fabrication and {Power} {Test} of the {Second} {MCBXFB} {Nested} {Orbit} {Corrector} {Prototype} for {HL}-{LHC}},
