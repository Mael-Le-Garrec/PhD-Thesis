@article{carlier_accuracy_2017,
	title = {Accuracy and {Feasibility} of the {Beta}* {Measurement} for {LHC} and {High} {Luminosity} {LHC} {Using} k {Modulation}},
	volume = {20},
	url = {https://link.aps.org/doi/10.1103/PhysRevAccelBeams.20.011005},
	doi = {10.1103/PhysRevAccelBeams.20.011005},
	abstract = {The future regimes of operation of the LHC will require improved control of \${\textbackslash}beta\_{\textbackslash}ast\$ measurements to successfully level the luminosities in the interaction points. The method of k modulation has been widely used in other machines to measure lattice beta functions. In the LHC, k modulation of the closest quadrupoles to the interaction point (IP) is the most accurate method to measure \${\textbackslash}beta\_{\textbackslash}ast\$ at the IP. This paper highlights the challenge of high precision tune measurements (down to 10-5) for the correct determination of \${\textbackslash}beta\_{\textbackslash}ast\$ in the high luminosity LHC. Furthermore it presents a new analytical method for the calculation of \${\textbackslash}beta\_{\textbackslash}ast\$ using k modulation.},
