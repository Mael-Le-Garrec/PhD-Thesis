@inproceedings{maclean_new_2017,
	title = {New {LHC} {Optics} {Correction} {Approaches} in 2017},
	url = {https://indico.cern.ch/event/663598/contributions/2781846},
	abstract = {The optics commissioning strategy for low-\${\textbackslash}beta\_{\textbackslash}ast\$ operation of the LHC underwent significant revision in 2017 [1]. The beam-based correction strategy transitioned from being concerned exclusively with linear optics errors, to a combined linear and nonlinear optics commissioning, with the traditional optics corrections at flat-orbit being both preceded and followed by corrections for the effect of nonlinear errors in the ATLAS and CMS insertions.},
