@article{maclean_measurement_2014,
	title = {Measurement of {Nonlinear} {Observables} in the {Large} {Hadron} {Collider} {Using} {Kicked} {Beams}},
	volume = {17},
	url = {https://link.aps.org/doi/10.1103/PhysRevSTAB.17.081002},
	doi = {10.1103/PhysRevSTAB.17.081002},
	abstract = {The nonlinear dynamics of a circular accelerator such as the Large Hadron Collider (LHC) may significantly impact its performance. As the LHC progresses to more challenging regimes of operation it is to be expected that the nonlinear single particle dynamics in the transverse planes will play an increasing role in limiting the reach of the accelerator. As such it is vital that the nonlinear sources are well understood. The nonlinear fields of a circular accelerator may be probed through measurement of the amplitude detuning: the variation of tune with single particle emittance. This quantity may be assessed experimentally by exciting the beam to large amplitudes with kicks, and obtaining the tunes and actions from turn-by-turn data at Beam Position Monitors. The large amplitude excitations inherent to such a measurement also facilitate measurement of the dynamic aperture from an analysis of beam losses following the kicks. In 2012 these measurements were performed on the LHC Beam 2 at injection energy (450 GeV) with the nominal magnetic configuration. Nonlinear coupling was also observed. A second set of measurements were performed following the application of corrections for b4 and b5 errors. Analysis of the experimental results, and a comparison to simulation are presented herein.},
	number = {8},
	urldate = {2018-03-07},
	journal = {Phys. Rev. ST Accel. Beams},
	author = {Maclean, Ewen Hamish and Tomás, Rogelio and Schmidt, Frank and Persson, Tobias Hakan Bjorn},
	month = aug,
	year = {2014},
	pages = {081002},
}

@phdthesis{maclean_modelling_2014,
	type = {{PhD} {Thesis}},
	title = {Modelling and {Correction} of the {Non}-{Linear} {Transverse} {Dynamics} of the {LHC} from {Beam}-{Based} {Measurements}},
	url = {http://cds.cern.ch/record/1951379},
	abstract = {The non-linear beam dynamics of a circular accelerator, such as the Large Hadron Collider, can have a significant impact on its operation. In order to avoid limitations on the performance reach of the accelerator, and ensure machine protection, it is vital that the beam dynamics are well understood and controlled. This thesis presents the results of studies of non-linear beam dynamics undertaken on the Large Hadron Collider at CERN, during the 2010 to 2013 period. It sets out to quantify the understanding of the non-linear beam dynamics through the comparison of beam-based measurements to simulation, and where able and appropriate seeks to explain deviations of measurement from the model, and define corrections for relevant aspects of the dynamics. The analyses presented in this thesis represent considerable advances in the understanding of the LHC beam dynamics which should allow for an improved operation of the machine in the coming years.},
	school = {Herford College, University of Oxford},
	author = {Maclean, Ewen Hamish},
	year = {2014},
	keywords = {to read},
}

