@inproceedings{soubelet_first_2022-1,
	address = {Bangkok, Thailand},
	title = {First {Interaction} {Region} {Local} {Coupling} {Corrections} in the {LHC} {Run} 3},
	copyright = {Creative Commons Attribution 4.0 International},
	isbn = {978-3-95450-227-1},
	url = {https://jacow.org/ipac2022/doi/JACoW-IPAC2022-WEPOPT007.html},
	doi = {10.18429/JACOW-IPAC2022-WEPOPT007},
	abstract = {The successful operation of large scale particle accelerators depends on the precise correction of unavoidable magnetic field or magnet alignment errors present in the machine. During the LHC Run 2, local linear coupling in the interaction regions (IR) was shown to have a significant impact on the beam size, making its proper handling a necessity for Run 3 and the High Luminosity LHC (HL-LHC). A new approach to accurately minimise the local IR linear coupling based on correlated external variables such as the {\textbar}C-{\textbar} had been proposed, which relies on the application of a rigid waist shift in order to create an asymmetry in the IR optics. In this contribution, preliminary corrections from the 2021 beam test and the early 2022 commissioning are presented, as well as first results of the new method’s experimental configuration tests in the LHC Run 3 commissioning.},
	language = {en},
	urldate = {2022-08-19},
