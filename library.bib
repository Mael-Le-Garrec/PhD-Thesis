@inproceedings{nebendahl_quality_2012,
	title = {Quality {Metrics} in {Optical} {Modulation} {Analysis}: {EVM} and {Its} {Relation} to {Q}-factor, {OSNR}, and {BER}},
	shorttitle = {Quality {Metrics} in {Optical} {Modulation} {Analysis}},
	abstract = {The quality of optical signals is a very important parameter in optical communications. Several metrics are in common use, like optical signal-to-noise power ratio (OSNR), Q-factor, error vector magnitude (EVM) and bit error ratio (BER). A measured raw BER is not necessarily useful to predict the final BER after soft-decision forward error correction (FEC), if the statistics of the noise leading to errors is unknown. In this respect the EVM is superior, as it allows an estimation of the error statistics. We compare various metrics analytically, by simulation, and through experiments. We employ six quadrature amplitude modulation (QAM) formats at symbol rates of 20 GBd and 25 GBd. The signals were generated by a software-defined transmitter. We conclude that for optical channels with additive Gaussian noise the EVM metric is a reliable quality measure. For nondata-aided QAM reception, BER in the range 10-6...10-2 can be reliably estimated from measured EVM.},
	booktitle = {2012 {Asia} {Commun}. {Photonics} {Conf}. {ACP}},
	author = {Nebendahl, B. and Schmogrow, R. and Dennis, T. and Josten, A. and Hillerkuss, D. and Koenig, S. and Meyer, J. and Dreschmann, M. and Winter, M. and Huebner, M. and Freude, W. and Koos, C. and Leuthold, J.},
	month = nov,
	year = {2012},
	keywords = {additive Gaussian noise, BER, bit error ratio, bit-error ratio (BER), error statistics, error vector magnitude, error vector magnitude (EVM), EVM, FEC, forward error correction, Gaussian noise, optical communication, optical communications, optical modulation, optical modulation analysis, optical signal-to-noise power ratio, OSNR, Q-factor, QAM, quadrature amplitude modulation, quality metrics, signal-to-noise power ratio, soft decision forward error correction, software defined transmitter, software-defined transmitter},
	pages = {1--3},
}

