@techreport{persson_experience_2016,
	title = {Experience with {DOROS} {BPMs} for {Coupling} {Measurement} and {Correction}},
	url = {http://cds.cern.ch/record/2207315},
	abstract = {The Diode ORbit and OScillation System (DOROS) system is designed to provide accurate measurements of the beam position in the LHC. The oscillation part of the system, which is able to provide turn-by-turn data, is used to measure the transverse coupling. Since the system provides high resolution measurements for many turns only small excitations are needed to accurately measure the transverse coupling. In this article we present the performance the system to measure coupling and compare it to the BPMs not equipped with this system.},
