@article{tomas_amplitude_2016,
	title = {Amplitude {Dependent} {Closest} {Tune} {Approach}},
	volume = {19},
	url = {https://link.aps.org/doi/10.1103/PhysRevAccelBeams.19.071003},
	doi = {10.1103/PhysRevAccelBeams.19.071003},
	abstract = {Recent observations in the LHC point to the existence of an amplitude dependent closest tune approach. However this dynamical behavior and its underlying mechanism remain unknown. This effect is highly relevant for the LHC as an unexpected closest tune approach varying with amplitude modifies the frequency content of the beam and, hence, the Landau damping. Furthermore the single particle stability would also be affected by this effect as it would modify how particles with varying amplitudes approach and cross resonances. We present analytic derivations that lead to a mechanism generating an amplitude dependent closest tune approach.},
	number = {7},
	urldate = {2018-03-07},
	journal = {Phys. Rev. Accel. Beams},
	author = {Tomás, R. and Persson, T. H. B. and Maclean, E. H.},
	month = jul,
	year = {2016},
	pages = {071003},
}

