	pages = {081003},
}

@techreport{persson_coupling_2016,
	type = {Accelerators \& {Technology} {Sector} {Note}},
	title = {Coupling {Measurements} and {Corrections} for the {Combined} {Ramp} and {Squeeze}},
	url = {https://cds.cern.ch/record/2135869},
	abstract = {In operation of the LHC the ramp and the squeeze process have been independent beam processes up to now. Making them into a combined process would save time to reach the point where the beams are brought to collision. This would increase the integrated luminosity provided by the LHC. One possible source of problems could be deviation from the ideal optics and in particular the control of the transverse coupling. In this report we focus on the coupling measurements that were taken during the Combined Ramp and Squeeze (CRS) MD.},
	number = {CERN-ACC-NOTE-2016-0030},
	urldate = {2018-05-28},
	author = {Persson, Tobias Hakan Bjorn and Maclean, Ewen Hamish and Tomas, Rogelio and Malina, Lukas and Skowronski, Piotr Krzysztof and Garcia-Tabares Valdivieso, Ana and Redaelli, Stefano and Vazquez, Coello De Portugal-Martinez and Maria, Jaime and Schaumann, Michaela and Gorzawski, Arkadiusz and Ferrando, Salvachua and Maria, Belen and Langner, Andy Sven and Wenninger, Jorg and Solfaroli Camillocci, Matteo},
