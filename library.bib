}

@techreport{brdokhalren_overview_2009,
	title = {Overview of {Magnetic} {Nonlinear} {Beam} {Dynamics} in {RHIC}},
	abstract = {Abstract In this article we review our studies of nonlinear beam dynamics due to the nonlinear magnetic field errors in the Relativistic Heavy Ion Collider (RHIC). Nonlinear mag\${\textbackslash}cdot\$-netic field errors, including magnetic field errors in interaction regions (IRs), chromatic sextupoles, and sextupole components from arc main dipoles are discussed. Their effects on beam dynamics and beam dynamic aperture are evaluated. The online methods to measure and correct the IR nonlinear field errors, second order chromaticities, and horizontal third order resonance are presented. The overall strategy for nonlinear corrections in RHIC is discussed.},
