@article{barranco_garcia_luminosity_2013-1,
	title = {Luminosity studies in a traveling waist regime in the {Compact} {Linear} {Collider}},
	volume = {16},
	url = {https://link.aps.org/doi/10.1103/PhysRevSTAB.16.041001},
	doi = {10.1103/PhysRevSTAB.16.041001},
	abstract = {The final focus system of the Compact Linear Collider (CLIC) features a crab cavity just before the final quadrupole doublet to ensure head-on collisions at the interaction point. A decrease in the expected luminosity with respect to the case without crossing angle has been recently observed in simulations for the CLIC baseline configuration. This effect is explained from the aberrations induced due to a z-dependent off-center horizontal orbit in the final focus sextupoles produced by the crab cavity. Three options to recover from this loss are explored. The effect of the longitudinal E−z correlation of the incoming beam from the linac is taken into account and its effect on the expected luminosity is evaluated.},
	number = {4},
	urldate = {2020-12-02},
	journal = {Physical Review Special Topics - Accelerators and Beams},
	author = {Barranco García, Javier and Marín Lacoma, Eduardo and Tomás García, Rogelio},
	month = apr,
	year = {2013},
	note = {Number: 4
Publisher: American Physical Society},
