@techreport{monig_simulations_2015,
	title = {Simulations of {Non}-{Linear} {Effects} in the {LHC}},
	abstract = {Simulations of accelerators allow the investigation of complex dynamics arising from non-linear components. Furthermore, simulations can be used as a tool for the analysis of new concepts. In the context of my summer student project, I did simulations of some non-linear effects in the LHC for comparison with recent measurements at maximal beam energy. I used the LHC model to analyse the dynamic aperture of the LHC with AC dipole, meaning with forced beam oscillations. This gives a deeper understanding of the beam dynamics with AC dipole. As result of these studies, the operation of the AC dipole can be improved. Moreover, I had the opportunity to participate in measurements on the LHC during MD.},
